<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='demo'>
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.user.theme.clean.Clean" />

	<!-- This is the library that we need :) -->
	<inherits name="androidemu" />

	<source path="demo" />

	<!-- Specify an Activity as entry point -->
	<entry-point class="android.demo.AndroidManifest" />

    <!-- Speed up compilation restricting to Webkit, Firefox and ie10 -->
    <set-property name="user.agent" value="safari,gecko1_8"/>
</module>